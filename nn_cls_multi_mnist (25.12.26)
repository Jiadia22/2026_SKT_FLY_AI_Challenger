        "def plot_history(history):\n",
        "  plt.plot(history.history['acc'], label = 'acc')\n",
        "  plt.plot(history.history['val_acc'], label = 'val_acc')\n",
        "  plt.xlabel(\"epoch\")\n",
        "  plt.ylabel(\"acc\")\n",
        "  plt.legend()\n",
        "  plt.grid(True)\n",
        "  plt.show()"
